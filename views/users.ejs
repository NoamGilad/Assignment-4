<%- include('includes/head.ejs') %>

<body>
  <%- include('includes/navigation.ejs') %>
  <main>
    <% if ($users.length > 0) { %>
    <ul>
      <% for (let user of $users) { %>
      <li>
        <p><%=user.name%></p>
      </li>
      <% } %>
    </ul>
    <% } else { %>
    <p>No Users Found!</p>
    <% } %>
  </main>
  <%- include('includes/end.ejs') %>
</body>
